{% extends 'CreditUnionFrontendBundle::layout.html.twig' %}
{% block title %}Додати нового клієнта{% endblock %}

{% block content %}   
<h1>Додати нового клієнта</h1>

<form action="{{ path('Customers_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Створити</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('Customers') }}">
            Назад до списку
        </a>
    </li>
</ul>
{% endblock %}