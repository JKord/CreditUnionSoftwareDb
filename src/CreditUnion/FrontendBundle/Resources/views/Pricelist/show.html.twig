<h1>Pricelist</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Кредина спілка</th>
            <td>{{ entity.creditunion.getName() }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ entity.type }}</td>
        </tr>
        <tr>
            <th>Nterestrate</th>
            <td>{{ entity.nterestrate }}</td>
        </tr>
        <tr>
            <th>Forthedate</th>
            <td>{{ entity.forthedate }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('Price') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('Price_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('Price_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
